<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.googry.upbacknavigationapplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity
            android:name=".Sample1Activity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Sample2Activity"
            android:parentActivityName=".MainActivity" />

        <activity android:name=".TestMainActivity" />
        <activity
            android:name=".Test1Activity"
            android:parentActivityName=".TestMainActivity" />
    </application>

</manifest>